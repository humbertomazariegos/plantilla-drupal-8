# Configuracion para Drupal 8
#
# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore paths that contain user-generated content.
/web/sites/*/files
/web/sites/*/private
/web/files/*

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php

# Note that services.local.yml is not loaded by default. If you would like
# to use this file add the following to your settings.local.php file:
#   $settings['container_yamls'][] = _DIR_ . '/services.local.yml';
/web/sites/*/services.local.yml

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

